    <script type="text/javascript">
        function isValidEmail(yes, regex = false) {
            if (yes) {
                $("#EmailId").removeClass("border-danger");
                $(".cross-validate").removeClass("error");
                $(".cross-validate").addClass("success");
                $(".warning-msg").hide();
            }
            else {
                $("#EmailId").addClass("border-danger");
                $(".cross-validate").removeClass("success");
                $(".cross-validate").addClass("error");
                if (regex)
                    $(".warning-msg").text("Enter a valid email");
                else
                    $(".warning-msg").text("This email is already in use");

                $(".warning-msg").show();
            }
        }

        function clearEmailValidate() {
            $("#EmailId").removeClass("border-danger");
            $(".cross-validate").removeClass("success");
            $(".cross-validate").removeClass("error");
            $(".warning-msg").hide();
        }

        async function IsEmailExist(email) {

            clearEmailValidate();

            const response = await fetch('/Subscribe/CheckEmailExist?email=' + email, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                }
            });
            return !response.ok;
        }

        $("#EmailId").on("blur", async function() {
            var emailregex = /^[a-zA-Z0-9.!#$%&'*+\/=?^_`{|}~-]+@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/;
            var email = $(this).val();

            if (email != "") {
                var matchArray = email.match(emailregex);
                if (matchArray != null) {
                    const exists = await IsEmailExist(email);
                    isValidEmail(!exists);
                }
                else
                    isValidEmail(false, true);
            }
            else
                clearEmailValidate();
        });

        $("#BtnSignupSubmit").on("click", async function(e) {
            e.preventDefault();
            var email = $("#EmailId").val();
            if (email != "") {
                const exists = await IsEmailExist(email);
                isValidEmail(!exists);

                if (exists)
                    return false;
                else
                    $("#signupForm").submit();
            }
        });
    </script>

</body>
</html>
